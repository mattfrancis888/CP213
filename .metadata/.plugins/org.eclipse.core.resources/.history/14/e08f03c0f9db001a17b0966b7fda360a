package exam;

public class hey {
	 public int[] permute(int n, int[] a) {
		 if(n == 1) {
			 System.out.println(a);
			 return a;
			} else {
			 for(int i = 0; i <= n; i ++) {
				 int temp = a[i];
				 a[i] = a[n];
				 //swap a[i] with a[n]
				 permute(n - 1, a);
				 //swap a[i] and [an]
				 a[i] = a[n];
			 }
		 }
	 }
	 public static void main(String[] args) {
		 hey test = new hey();
		 int a[] = {0,7,8,9};
		 test.permute(3, a);
	 }
}
